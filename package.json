{
  "name": "CHEEMOS",
  "author": "Abhishek <abhishek@engx.com>",
  "description": "AI agent powered by LLM.",
  "version": "2.0.0",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.4",
    "@mui/joy": "^5.0.0-beta.22",
    "@mui/material": "^5.15.4",
    "@react-oauth/google": "^0.12.1",
    "@reduxjs/toolkit": "^2.4.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "antd": "^5.22.2",
    "dotenv": "^16.4.1",
    "electron-is-dev": "^2.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-icons": "^5.3.0",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.28.0",
    "react-scripts": "5.0.1",
    "react-scroll-to-bottom": "^4.2.0",
    "react-syntax-highlighter": "^15.6.1",
    "reactflow": "^11.11.4",
    "redux": "^5.0.1",
    "rehype-katex": "^7.0.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "concurrently --raw 'npm run dev:react' 'wait-on http://localhost:3000 && npm run dev:app'",
    "build": "npm run pack:react",
    "dev:react": "GENERATE_SOURCEMAP=false react-scripts start",
    "dev:app": "electron .",
    "pack:react": "rm -rf ./build && react-scripts build",
    "pack:app": "rm -rf ./dist && electron-builder -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "electron": "^28.1.3",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.electron.friday",
    "productName": "FRIDAY",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": [
        {
          "target": "portable"
        },
        {
          "target": "nsis"
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "appImage"
        },
        {
          "target": "deb"
        }
      ],
      "category": "Utility"
    },
    "mac": {
      "target": "dmg"
    }
  }
}
